{
  "name": "ai-web-automation",
  "version": "1.0.0",
  "description": "AI-driven web automation system using Chrome Recorder JSON",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:bundle": "node build-bundle.js",
    "build:all": "npm run build && npm run build:bundle",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "deploy": "node deploy.js",
    "package": "npm run build && npm run deploy",
    "package:bundle": "npm run build:bundle && npm run deploy"
  },
  "keywords": [
    "automation",
    "playwright",
    "ai",
    "web-scraping"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/ws": "^8.18.1",
    "axios": "^1.13.2",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "openai": "^4.20.0",
    "pg": "^8.11.3",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.40.0",
    "playwright-extra": "^4.3.6",
    "prom-client": "^15.1.0",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.32.0",
    "tesseract.js": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "esbuild": "^0.27.1",
    "eslint": "^8.54.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.15"
  }
}
